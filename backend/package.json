{
  "name": "karkowka-backend",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "watch": "nodemon src/main.ts --watch 'src/**/*'",
    "build": "tsc",
    "start" :"node dist/main.js",
    "migrate:initial": "mikro-orm migration:create --initial",
    "migrate": "mikro-orm migration:create",
    "migrate-start": "mikro-orm migration:create && tsc && node dist/main.js"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/bcrypt": "^5.0.0",
    "@types/connect-redis": "^0.0.18",
    "@types/express": "^4.17.13",
    "@types/express-session": "^1.17.4",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "nodemon": "^2.0.15",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@mikro-orm/cli": "^5.1.1",
    "@mikro-orm/core": "^5.1.1",
    "@mikro-orm/migrations": "^5.1.1",
    "@mikro-orm/postgresql": "^5.1.1",
    "@mikro-orm/reflection": "^5.1.1",
    "bcrypt": "^5.0.1",
    "connect-redis": "^6.1.3",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "lodash": "^4.17.21",
    "redis": "^4.0.4"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
